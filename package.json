{
  "name": "react-mosaic-component",
  "version": "0.4.0",
  "description": "A React Tiling Window Manager",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "style": "lib/react-mosaic.css",
  "typings": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitaliipetrenko/react-mosaic.git"
  },
  "scripts": {
    "clean": "rm -rf lib/",
    "start": "webpack-dev-server --config webpack.config.hot.js --inline --hot --content-base docs/ --host 0.0.0.0 --port 8092",
    "build": "npm-run-all clean lib.compile.ts lib.compile.less lib.remove.type.references",
    "docs": "rm -rf docs/ && webpack",
    "test": "tslint --project tsconfig.json && mocha --opts test/mocha.opts 'test/*.ts'",
    "lib.compile.ts": "tsc -p tsconfig-build.json",
    "lib.compile.less": " lessc --source-map styles/index.less react-mosaic-component.css",
    "lib.remove.type.references": "replace '^/// <reference.+?\\n' '' lib -r --include='*.d.ts'"
  },
  "devDependencies": {
    "@blueprintjs/core": "^1.18.0",
    "@types/assertion-error": "^1.0.30",
    "@types/chai": "^3.5.2",
    "@types/classnames": "^2.2.0",
    "@types/es6-promise": "^0.0.32",
    "@types/immutability-helper": "^2.0.15",
    "@types/lodash": "^4.14.64",
    "@types/mocha": "^2.2.41",
    "@types/pure-render-decorator": "^0.2.27",
    "@types/react": "^15.0.25",
    "@types/react-dnd": "^2.0.31",
    "@types/react-dnd-html5-backend": "^2.1.6",
    "@types/react-dom": "^15.5.0",
    "@types/uuid": "^2.0.29",
    "chai": "^4.0.0",
    "css-loader": "^0.28.4",
    "file-loader": "^0.11.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "jsdom": "^11.0.0",
    "jsdom-global": "^3.0.2",
    "less": "^2.6.0",
    "less-loader": "^4.0.4",
    "mocha": "^3.4.2",
    "mocha-junit-reporter": "^1.12.1",
    "npm-run-all": "^4.0.0",
    "react": "^15.5.4",
    "react-addons-css-transition-group": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-hot-loader": "^1.3.1",
    "replace": "v0.3.0",
    "source-map-loader": "^0.2.1",
    "style-loader": "^0.18.1",
    "ts-loader": "^2.1.0",
    "ts-node": "^3.0.4",
    "tslint": "^5.3.2",
    "typescript": "^2.3.4",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "immutability-helper": "^2.2.2",
    "lodash": "^4.17.2",
    "pure-render-decorator": "^1.2.1",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
    "uuid": "^3.0.1"
  },
  "peerDependencies": {
    "react": "^15.0.0 || ^0.14.0"
  },
  "keywords": [
    "ui",
    "palantir",
    "react",
    "component",
    "typescript",
    "tiling-window-manager",
    "window-manager"
  ]
}
